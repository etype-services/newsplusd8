{
  "name": "drupal-composer/drupal-project",
  "description": "Project template for Drupal 8 projects with composer",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "",
      "role": ""
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "package",
      "package": {
        "name": "woothemes/flexslider",
        "version": "2.6.3",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/woothemes/FlexSlider.git",
          "type": "git",
          "reference": "2.6.3"
        }
      }
    }
  ],
  "require": {
    "ext-PDO": "^7.1",
    "ext-SimpleXML": "*",
    "ext-libxml": "*",
    "ext-soap": "*",
    "ext-zip": "*",
    "composer/installers": "^1.2",
    "cweagans/composer-patches": "^1.6",
    "drupal-composer/drupal-scaffold": "^2.2",
    "drupal/addtoany": "^1.12",
    "drupal/admin_toolbar": "^2.0",
    "drupal/adstxt": "^1.0",
    "drupal/block_class": "^1.1",
    "drupal/ckeditor_font": "^1.0",
    "drupal/clamav": "^1.0",
    "drupal/colorbutton": "^1.1",
    "drupal/conditional_styles": "^1.0",
    "drupal/console": "~1.0",
    "drupal/core": "^8.8@beta",
    "drupal/custom_pub": "^1.0",
    "drupal/delete_all": "^1.1",
    "drupal/devel": "^2.0",
    "drupal/dfp": "^1.0",
    "drupal/editor_file": "^1.4",
    "drupal/entity_embed": "^1.0",
    "drupal/entity_reference_revisions": "^1.6",
    "drupal/exclude_node_title": "^1.0",
    "drupal/features": "^3.7",
    "drupal/field_tools": "^1.0",
    "drupal/flexslider": "^2.0",
    "drupal/fontawesome": "^2.9",
    "drupal/fullcalendar_view": "^2.3",
    "drupal/git_deploy": "^2.0",
    "drupal/google_analytics": "^3.0",
    "drupal/honeypot": "^1.29",
    "drupal/ie9": "^1.0",
    "drupal/image_effects": "^2.3",
    "drupal/imce": "^1.7",
    "drupal/inline_responsive_images": "^2.1",
    "drupal/jquery_view_ticker": "^1.0",
    "drupal/mailchimp": "^1.9",
    "drupal/metatag": "^1.8",
    "drupal/migrate_plus": "^4.2",
    "drupal/migrate_tools": "^4.1",
    "drupal/migrate_upgrade": "^3.0",
    "drupal/paragraphs": "^1.6",
    "drupal/pathauto": "^1.3",
    "drupal/poll": "^1.2",
    "drupal/rebuild_cache_access": "^1.4",
    "drupal/recaptcha": "^2.4",
    "drupal/redirect": "^1.3",
    "drupal/scheduler": "^1.0",
    "drupal/simpleads": "1.x-dev",
    "drupal/smtp": "^1.0",
    "drupal/social_media": "^1.3",
    "drupal/superfish": "^1.3",
    "drupal/taxonomy_import": "^1.0",
    "drupal/taxonomy_menu_ui": "^1.0",
    "drupal/token": "^1.5",
    "drupal/twig_tweak": "^2.1",
    "drupal/ultimate_cron": "^2.0",
    "drupal/upgrade_status": "^2.0",
    "drupal/video_embed_field": "^2.1",
    "drupal/view_unpublished": "^1.0",
    "drupal/views_bulk_operations": "^3.2",
    "drupal/views_database_connector": "^1.1",
    "drupal/views_infinite_scroll": "^1.6",
    "drupal/weather_block": "^1.1",
    "drupal/webform": "^5.0",
    "drush/drush": "^10.0",
    "vlucas/phpdotenv": "^2.4",
    "webflo/drupal-finder": "^1.0.0",
    "webmozart/path-util": "^2.3",
    "woothemes/flexslider": "~2.0"
  },
  "require-dev": {
    "drupal/core-dev": "^8.8@beta",
    "friendsofphp/php-cs-fixer": "^2.12",
    "symfony/debug": "^3.4"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ],
    "files": [
      "load.environment.php"
    ]
  },
  "scripts": {
    "pre-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "pre-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "cs-fix": [
      "php-cs-fixer fix src --rules=@PSR2"
    ]
  },
  "extra": {
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/Commands/{$name}": [
        "type:drupal-drush"
      ]
    },
    "drupal-scaffold": {
      "initial": {
        ".editorconfig": "../.editorconfig",
        ".gitattributes": "../.gitattributes"
      }
    }
  }
}
