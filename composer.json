{
  "name": "drupal-composer/drupal-project",
  "description": "Project template for Drupal 8 projects with composer",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "",
      "role": ""
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "package",
      "package": {
        "name": "woothemes/flexslider",
        "version": "2.6.3",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/woothemes/FlexSlider.git",
          "type": "git",
          "reference": "2.6.3"
        }
      }
    }
  ],
  "require": {
    "ext-SimpleXML": "*",
    "ext-libxml": "*",
    "ext-zip": "*",
    "composer/installers": "^1.2",
    "cweagans/composer-patches": "^1.6",
    "drupal-composer/drupal-scaffold": "^2.2",
    "drupal/addtoany": "^1.12",
    "drupal/admin_toolbar": "^1.26",
    "drupal/clamav": "^1.0",
    "drupal/console": "^1.0.2",
    "drupal/core": "^8.6.0",
    "drupal/custom_pub": "^1.0",
    "drupal/devel": "^2.0",
    "drupal/dfp": "^1.0",
    "drupal/entity_reference_revisions": "^1.6",
    "drupal/exclude_node_title": "^1.0",
    "drupal/features": "^3.7",
    "drupal/flexslider": "^2.0",
    "drupal/fontawesome": "^2.9",
    "drupal/google_analytics": "^3.0",
    "drupal/honeypot": "^1.29",
    "drupal/imce": "^1.7",
    "drupal/paragraphs": "^1.6",
    "drupal/pathauto": "^1.3",
    "drupal/poll": "^1.2",
    "drupal/recaptcha": "^2.4",
    "drupal/smtp": "^1.0",
    "drupal/social_media": "^1.3",
    "drupal/superfish": "^1.3",
    "drupal/taxonomy_import": "^1.0",
    "drupal/token": "^1.5",
    "drupal/twig_tweak": "^2.1",
    "drupal/ultimate_cron": "^2.0",
    "drupal/views_database_connector": "^1.1",
    "drupal/views_slideshow": "^4.6",
    "drupal/weather_block": "^1.1",
    "drupal/webform": "^5.0",
    "drush/drush": "^9.5",
    "twig/twig": "^1.38.2",
    "vlucas/phpdotenv": "^2.4",
    "webflo/drupal-finder": "^1.0.0",
    "webmozart/path-util": "^2.3",
    "woothemes/flexslider": "~2.0"
  },
  "require-dev": {
    "webflo/drupal-core-require-dev": "^8.6.0",
    "friendsofphp/php-cs-fixer": "^2.12"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ],
    "files": [
      "load.environment.php"
    ]
  },
  "scripts": {
    "pre-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "pre-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "cs-fix": [
      "php-cs-fixer fix src --rules=@PSR2"
    ]
  },
  "extra": {
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/Commands/{$name}": [
        "type:drupal-drush"
      ]
    },
    "drupal-scaffold": {
      "initial": {
        ".editorconfig": "../.editorconfig",
        ".gitattributes": "../.gitattributes"
      }
    }
  }
}
